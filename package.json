{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "gatsby": "^2.19.24",
    "gatsby-image": "^2.2.42",
    "gatsby-plugin-dark-mode": "^1.1.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.2.42",
    "gatsby-plugin-offline": "^3.0.35",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-sass": "^2.1.29",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-typescript": "^2.4.16",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-sharp": "^2.3.16",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-visibility-sensor": "^5.1.1"
  },
  "devDependencies": {
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.7",
    "@types/node": "^14.0.25",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "3.7.1",
    "@typescript-eslint/parser": "3.7.1",
    "axe-core": "^3.5.5",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.2.2",
    "babel-preset-gatsby": "^0.5.4",
    "cypress": "^4.11.0",
    "cypress-axe": "^0.8.1",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-typescript": "9.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-import-resolver-typescript": "2.2.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-react-hooks": "4.0.8",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.2.2",
    "lint-staged": "^10.2.11",
    "prettier": "1.19.1",
    "prettier-eslint": "11.0.0",
    "prettier-eslint-cli": "5.0.0",
    "react-test-renderer": "^16.13.1",
    "start-server-and-test": "^1.11.2",
    "typescript": "^3.9.7"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint . --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,scss,json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:e2e:ci:develop && lint-staged && npm run test"
    }
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,scss,json,md}\" && eslint --fix --ext .ts --ext .tsx .",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest --coverage --colors",
    "lint-eslint": "eslint --ext .ts --ext .tsx .",
    "lint-prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx,scss,json,md}\"",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci:develop": "start-server-and-test develop http://localhost:8000 cy:run",
    "test:e2e:ci:serve": "start-server-and-test serve http://localhost:9000 'CYPRESS_baseUrl=http://localhost:9000 npm run cy:run'"

  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}

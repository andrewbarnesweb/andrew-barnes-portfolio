{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "gatsby": "^2.32.3",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-dark-mode": "^1.1.2",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.12.0",
    "gatsby-plugin-offline": "^3.10.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-sass": "^3.2.0",
    "gatsby-plugin-sharp": "^2.14.1",
    "gatsby-plugin-typescript": "^2.12.0",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-transformer-sharp": "^2.12.0",
    "node-sass": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-visibility-sensor": "^5.1.1"
  },
  "devDependencies": {
    "@percy/cypress": "^2.3.4",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/node": "^14.14.28",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "axe-core": "^4.1.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby": "^0.12.1",
    "cypress": "^6.4.0",
    "cypress-axe": "^0.12.1",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "prettier-eslint": "12.0.0",
    "prettier-eslint-cli": "5.0.0",
    "react-test-renderer": "^17.0.1",
    "start-server-and-test": "^1.12.0",
    "typescript": "^4.1.5"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint . --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,scss,json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:e2e:ci:develop && lint-staged && npm run test"
    }
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,scss,json,md}\" && eslint --fix --ext .ts --ext .tsx .",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest --coverage --colors",
    "lint-eslint": "eslint --ext .ts --ext .tsx .",
    "lint-prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx,scss,json,md}\"",
    "cy:open": "cypress open",
    "cy:run": "percy exec -- cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci:develop": "start-server-and-test develop http://localhost:8000 cy:run",
    "test:e2e:ci:serve": "start-server-and-test serve http://localhost:9000 'CYPRESS_baseUrl=http://localhost:9000 npm run cy:run'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
